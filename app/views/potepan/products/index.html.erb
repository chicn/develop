<!-- MAIN CONTENT SECTION -->
<section class="mainContent clearfix productsContent">
  <div class="container">
    <div class="row">
      <div class="col-md-3 col-sm-4 col-xs-12 sideBar">
        <div class="panel panel-default">
          <div class="panel-heading">商品カテゴリー</div>
          <div class="panel-body">
            <div class="collapse navbar-collapse navbar-ex1-collapse navbar-side-collapse">
              <ul class="nav navbar-nav side-nav">
                <% @prototypes.each do |p| %>
                  <li>
                    <a href="javascript:;" data-toggle="collapse" data-target="#SHOES"><%= p.name %><i class="fa fa-plus"></i></a>
                    <ul id="SHOES" class="collapse collapseItem">
                      <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>サブカテゴリー名 <span>(6)</span></a></li>
                      </ul>
                  </li>
                <% end %>
                <!-- <li>
                  <a href="javascript:;" data-toggle="collapse" data-target="#CLOTHING">CLOTHING<i class="fa fa-plus"></i></a>
                  <ul id="CLOTHING" class="collapse collapseItem">
                    <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>サブカテゴリー名 <span>(6)</span></a></li>
                  </ul>
                </li> -->
              </ul>
            </div>
          </div>
        </div>
        <div class="panel panel-default priceRange">
